package array1;

import java.util.Arrays;

public class Bubble {

	public static void main(String[] args) {
		int a[]= {10,2,6,7,8,5,3,4,9,1};
		
		for(int i=0;i<a.length-1;i++)
		{
			if(a[i]>a[i+1])
			{
				int tmp=a[i];
				a[i]=a[i+1];
				a[i+1]=tmp;
			}
		}
		System.out.println(Arrays.toString(a));
	}

}
