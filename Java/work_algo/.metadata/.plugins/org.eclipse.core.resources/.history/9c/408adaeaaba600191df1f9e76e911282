package array1;

import java.util.Arrays;
import java.util.Random;
import java.util.Scanner;

public class ForTest {
	public static void main(String[] args) {
		int[] ia= {10};
		Random r = new Random();
	
		
		
		int sum=0;
		for(int i=0;i<=9;i++)
		{
			ia[i]=r.nextInt(100)+1;
			sum=sum+ia[i];
			
		}
		System.out.println("sum="+sum);
	}

}

