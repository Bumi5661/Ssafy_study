import java.util.Scanner;

public class move {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String[][] arr= new String[6][6];
		int cnt=0;
		
		//배열 구현
		for(int i=0;i<6;i++)
		{
			for(int j=0;j<6;j++)
			{
				arr[i][j]=sc.next();
			}
		}
		
		
		for(int i=0;i<6;i++)
		{
			int idxa=0;
			for(int j=0;j<6;j++)
			{
				if(arr[i][j]=="A")
				{
					idxa=j;
					
				}
			}
		}
	}	

}
